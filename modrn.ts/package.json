  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/chai": "^4.2.21",
    "@types/jasmine": "^3.8.1",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "autoprefixer": "^10.3.1",
    "browserify": "^17.0.0",
    "chai": "^4.3.4",
    "chokidar": "^3.5.2",
    "chokidar-cli": "^2.1.0",
    "concurrently": "^6.2.0",
    "cp-cli": "^2.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.30.0",
    "jasmine": "^3.8.0",
    "karma": "^6.3.4",
    "karma-chai": "^0.1.0",
    "karma-firefox-launcher": "^2.1.1",
    "karma-jasmine": "^4.0.1",
    "karma-typescript": "^5.5.1",
    "mkdirp": "^1.0.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.55.1",
    "rollup-plugin-dts": "^3.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "shx": "^0.3.3",
    "tinyify": "^3.0.0",
    "tsify": "^5.0.4",
    "typedoc": "^0.21.5",
    "typedoc-neo-theme": "^1.1.1",
    "typedoc-plugin-markdown": "^3.10.4",
    "typescript": "^4.3.5",
    "watchify": "^4.0.0"
  },
  "scripts": {
    "test": "karma start",
    "clean": "rimraf output/* && mkdirp output",
    "lint": "npx eslint js/**/*.ts",
    "create-dist-folder": "rimraf dist && mkdirp dist",
    "copy-to-dist-folder": "shx cp output/modrn.* dist/",
    "copy-readme-to-dist-folder": "shx cp ../*.md dist/",
    "copy-package.json": "shx cp dist-package-json.json dist/package.json",
    "build-js": "cross-env NODE_ENV=production npx rollup -c ./rollup.config.js",
    "build-index-ts": "cross-env NODE_ENV=production npx rollup -c ./rollup-dts.config.js",
    "build": "npm run lint && npm run clean && npm run build-js && npm run create-dist-folder && npm run copy-to-dist-folder && npm run build-index-ts && npm run copy-package.json && npm run copy-readme-to-dist-folder && cd dist && npm pack"
  },
  "_id": "Modrn@0.1.7",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexbfr/modrn.git"
  }
